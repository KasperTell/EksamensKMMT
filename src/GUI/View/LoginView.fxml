<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="acpBackgroundLogin" prefHeight="800" prefWidth="1200" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.LoginController">
   <children>
      <ImageView fx:id="ImgViewLogin" fitHeight="800.0" fitWidth="1200.0" />
      <PasswordField fx:id="paswPassword" layoutX="521.0" layoutY="429.0" promptText="Password" />
      <TextField fx:id="txtfUsername" layoutX="520.0" layoutY="363.0" promptText="Username" />
      <Button fx:id="btnSignIn" layoutX="568.0" layoutY="512.0" mnemonicParsing="false" onAction="#handleSignIn" text="Sign in" />
   </children>
</AnchorPane>
